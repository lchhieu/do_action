{
    "name": "lchhieu/do_action",
    "description": "Do action package invokes all functions attached to action hook in Laravel",
    "keywords": ["do_action", "laravel", "package"],
    "homepage": "https://github.com/lchhieu/do_action",
    "license" : "MIT",
    "authors": [
        {
            "name": "Hieu Luong",
            "email": "luongconghieu93@gmail.com"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": ">=5.3"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "Lchhieu\\DoAction\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lchhieu\\DoAction\\Providers\\DoActionServiceProvider"
            ],
            "aliases": {
                "Actions": "Lchhieu\\DoAction\\Facades\\Actions",
                "Filter": "Lchhieu\\DoAction\\Facades\\Filter"
            }
        }
    }
}
